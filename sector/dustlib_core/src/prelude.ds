// dustlib prelude helpers.

forge DustlibPrelude {

    proc K::init() -> UInt32 {
        emit "dustlib prelude initialized";
        return 0;
    }

    proc K::print_ok(message: UInt64) -> UInt32 {
        puts(message);
        putchar(10);
        return 0;
    }

    proc K::assert_true(value: UInt32) -> UInt32 {
        if value == 0 {
            emit "prelude assert failed";
            return 1;
        } else {
            return 0;
        }
    }
}
